<ul class="contacts {{ bemMix }}">
	{% for contact in contacts %}
		<li class="contacts__item">
			<a class="contacts__link contacts__link--{{ contact.id }}" href="{{ contact.id }}:{{ contact.value }}">
				<span class="contacts__text">{{ contact.title ?? contact.value }}</span>
			</a>
		</li>
	{% endfor %}
</ul>
